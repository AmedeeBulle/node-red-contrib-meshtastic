    "eslint-plugin-prettier": "^4.2.1",
    "node-red": "^3.0.2",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
